{{#each this}}
  <div>
    <h2 class='country-name'>{{name}}</h2>
    <div class='country-data'>
      <p class='country-data-caption'>Capital:
        <span class='caption-wrapper'>{{capital}}</span>
      </p>
      <p class='country-data-caption'>Population:
        <span class='caption-wrapper'>{{population}}</span>
      </p>
      <p class='country-data-caption'>Languages:
        <span class='caption-wrapper'></span>
      </p>
      <ul class='list-group'>
        {{#each languages}}
          <li class='list-group-item'>{{this.name}}</li>
        {{/each}}
      </ul>
    </div>
    <div class='image-wrapper'>
      <img src='{{flags.png}}' alt='{{name}}' class='country-flag' />
    </div>
  </div>
{{/each}}